# Server Configuration
NODE_ENV=development
PORT=5000

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=juggernaut_idv
DB_USER=postgres
DB_PASSWORD=postgres
DB_SSL=false

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_TTL=3600

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRES_IN=24h
JWT_REFRESH_SECRET=your-refresh-secret-key-change-this
JWT_REFRESH_EXPIRES_IN=7d

# API Keys
MASTER_API_KEY=jug_master_key_change_this_in_production
SYSTEM_API_KEY=jug_system_key_change_this_in_production

# Cryptography
CRYPTO_SECRET_KEY=your32bytesecretkeychangethisnow

# CORS
CORS_ORIGINS=http://localhost:3000,http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX=100

# File Upload
MAX_FILE_SIZE=10485760

# Logging
LOG_LEVEL=info
LOG_FORMAT=json
LOG_DIR=./logs

# Email Service
EMAIL_SERVICE_ENABLED=false
EMAIL_API_KEY=
EMAIL_FROM=noreply@juggernaut.ai

# Webhook
WEBHOOK_URL=

# Blockchain
BLOCKCHAIN_ENABLED=false
BLOCKCHAIN_NETWORK=testnet
BLOCKCHAIN_RPC_URL=
CONTRACT_ADDRESS=
BLOCKCHAIN_PRIVATE_KEY=

# Feature Flags
ENABLE_GAMING_DETECTION=true
ENABLE_AUTONOMY_RECOMPUTATION=true
ENABLE_CHAIN_VERIFICATION=true
ENABLE_RECEIPT_VERIFICATION=true
ENABLE_METRICS=false
ENABLE_ANALYTICS=false

# Performance
MAX_WORKERS=4
QUEUE_CONCURRENCY=10
BATCH_SIZE=100